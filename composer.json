{
    "name": "ticketpark/doctrine-fixtures-autoloader",
    "type": "library",
    "description": "Simplifies loading doctrine fixtures",
    "keywords": ["doctrine", "fixtures", "database"],
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Ticketpark contributors and others",
            "homepage": "https://github.com/Ticketpark/TicketparkFixturesAutoloadBundle/contributors"
        }
    ],

    "require": {
        "php": ">=5.3.3",
        "doctrine/data-fixtures": "^1.1"
    },

    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0"
    },

    "autoload": {
        "psr-4": { "Ticketpark\\Doctrine\\": "lib/Ticketpark/Doctrine" }
    },
    "autoload-dev": {
        "psr-4": { "Ticketpark\\Tests\\": "tests/Ticketpark" }
    }
}
